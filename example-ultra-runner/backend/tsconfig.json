{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ESNext"],
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "generated/*": ["../definition/generated/*"]
    }
  }
}
